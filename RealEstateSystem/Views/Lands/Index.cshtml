@model IEnumerable<RealEstateSystem.Models.Land>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IsTimberLand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Length)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Width)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WaterWell)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WaterWellAtNear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LandForConstruction)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LandIsArable)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EarthType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Property)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IsTimberLand)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Length)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Width)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WaterWell)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WaterWellAtNear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LandForConstruction)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LandIsArable)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EarthType.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Property.Description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
